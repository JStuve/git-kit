{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run TS File",
            "args": ["${relativeFile}"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": ["--loader", "ts-node/esm"],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}